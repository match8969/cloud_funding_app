<p id="notice"><%= notice %></p>

<table>
  <thead>
  <tr>
    <th>Product title</th>
    <th>Owner User Name</th>
    <th>Goal Price</th>
    <th>Current Price</th>

    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% products.each do |product| %>
    <tr>
      <td><%= product.title %></td>
      <td><%= product.user.name %></td>
      <td><%= product.goal_price %></td>
      <td><%= product.get_current_price %></td>
    </tr>
  <% end %>
  </tbody>
</table>